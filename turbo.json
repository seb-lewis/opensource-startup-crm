{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"dev": {
			"cache": false,
			"persistent": true,
			"dependsOn": ["^build", "^db:gmb:local"]
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".svelte-kit/**"]
		},
		"db:generate": {
			"cache": false
		},
		"db:migrate:local": {
			"cache": false
		},
		"db:migrate:prod": {
			"cache": false
		},
		"db:gmb:local": {
			"cache": false
		},
		"db:gmb:prod": {
			"cache": false
		},
		"db:studio": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"dependsOn": ["build"]
		}
	}
}
